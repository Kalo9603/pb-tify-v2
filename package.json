{
  "name": "@teipublisher/pb-tify-v2",
  "version": "1.0.0",
  "description": "An example of a possible pb-tify extension.",
  "main": "pb-tify-v2.js",
  "scripts": {
    "start": "npm run build && web-dev-server --config web-dev-server.config.mjs",
    "docs": "wca src -f json --outFile custom-elements.json",
    "build": "npm run clean && npm run docs && rollup -c rollup.config.js",
    "build:production": "npm run clean && npm run docs && rollup -c rollup.config.js --environment BUILD:production",
    "clean": "rimraf dist i18n/common images css lib custom-elements.json",
    "prepare": "npm run docs && npm run build:production"
  },
  "author": "Calogero Giudice",
  "license": "GPL-3.0-or-later",
  "repository": "https://github.com/Kalo9603/pb-tify-v2/tree/test",
  "dependencies": {
    "@teipublisher/pb-components": "latest",
    "dompurify": "^3.2.6",
    "tailwind-colors-vars": "^1.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@web/dev-server": "^0.4.6",
    "autoprefixer": "^10.4.21",
    "es-dev-server": "^1.48.1",
    "postcss": "^8.5.6",
    "rimraf": "^3.0.2",
    "rollup": "^2.8.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-terser": "^5.3.0",
    "tailwindcss": "^4.1.11",
    "web-component-analyzer": "^1.0.3"
  }
}
